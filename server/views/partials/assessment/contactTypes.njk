{% from "govuk/components/table/macro.njk" import govukTable %}
{% macro contactTypes(contact) %}
    <span data-qa="{{contact.userRole}}">
    {% switch contact.userRole %}
    {% case 'PROBATION_COM' %}
        Probation practitioner
    {% case 'PRISON_DM' %}
        Decision maker
    {% case 'PRISON_CA' %}
        Case administrator
    {% default %}
       Contact
    {% endswitch %}
    </span>
{% endmacro %}
